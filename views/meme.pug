html
    style
        include style.css
    body
        img(src=meme.url)
        h1="Price history"
        table
            each price in meme.prices
                tr
                    td= price
        form(action="http://localhost:3000/meme/" + meme.id method="post")
            input(type="text" name="price")
            input(type="submit", value="Set")
        a(href="/") back